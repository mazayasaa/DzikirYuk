<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vokasi.dzikiryuk">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DzikirPagiDetails">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vokasi.dzikiryuk.MenuDzikirPagi" />
        </activity>
        <activity android:name=".MenuDzikirPagi" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vokasi.dzikiryuk.MainActivity" />
        </activity>
        <activity android:name=".DzikirPetangDetails">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vokasi.dzikiryuk.MenuDzikirPetang" />
        </activity>
        <activity android:name=".MenuDzikirPetang">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vokasi.dzikiryuk.MainActivity" />
        </activity>
        <activity android:name=".MenuTasbih">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.vokasi.dzikiryuk.MainActivity" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>